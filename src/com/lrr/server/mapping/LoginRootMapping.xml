<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lrr.server.mapping.LoginRootMapper">

	<!-- 用户登录查询 -->
	<select id="findLoginRoot" parameterType="String"
		resultType="com.lrr.server.entity.LoginRoot">
		select * from LoginRoot where rootId=#{rootId} and password=#{password}
	</select>


</mapper>